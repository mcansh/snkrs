{
  "name": "@mcansh/snkrs",
  "version": "1.0.0",
  "description": "my sneaker collection",
  "repository": "mcansh/snkrs",
  "license": "MIT",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "main": "n/a",
  "prettier": "@mcansh/eslint-config/prettier.config",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && npm run generate"
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "generate": "prisma generate",
    "lint": "eslint --cache --ext .js,.ts,.tsx .",
    "test": "jest",
    "type-check": "tsc",
    "validate": "npm-run-all --parallel lint type-check test",
    "db:save": "prisma migrate save --experimental",
    "db:up": "prisma migrate up --experimental",
    "db:generate": "prisma generate",
    "db:migrate": "npm-run-all db:*",
    "find:unused": "mkdir pages && next-unused && rm -r pages"
  },
  "next-unused": {
    "alias": {},
    "include": [
      "src"
    ],
    "exclude": [],
    "entrypoints": [
      "src/pages"
    ]
  },
  "dependencies": {
    "@nexus/schema": "0.14.0",
    "@prisma/client": "2.1.3",
    "@svgr/webpack": "5.4.0",
    "argon2": "0.26.2",
    "cloudinary-core": "2.9.0",
    "date-fns": "2.14.0",
    "formik": "2.1.4",
    "next": "9.4.4",
    "next-iron-session": "4.1.7",
    "next-offline": "5.0.2",
    "next-seo": "4.6.0",
    "npm": "6.14.6",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-simple-img": "2.3.9",
    "swr": "0.2.3",
    "type-fest": "0.16.0",
    "yup": "0.29.1"
  },
  "devDependencies": {
    "@commitlint/cli": "9.0.1",
    "@commitlint/config-conventional": "9.0.1",
    "@mcansh/eslint-config": "2.7.3",
    "@next/eslint-plugin-next": "9.4.5-canary.22",
    "@prisma/cli": "2.1.3",
    "@tailwindcss/ui": "0.3.0",
    "@testing-library/jest-dom": "5.11.0",
    "@testing-library/react": "10.4.3",
    "@types/argon2": "0.15.0",
    "@types/jest": "26.0.3",
    "@types/node": "14.0.14",
    "@types/react": "16.9.41",
    "@types/react-dom": "16.9.8",
    "@types/yup": "0.29.3",
    "@typescript-eslint/eslint-plugin": "3.4.0",
    "@typescript-eslint/parser": "3.4.0",
    "eslint": "7.3.1",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-kentcdodds": "14.14.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.10.0",
    "husky": "4.2.5",
    "jest": "26.1.0",
    "lint-staged": "10.2.11",
    "next-unused": "0.0.2",
    "npm-run-all": "4.1.5",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.0.5",
    "shell-quote": "1.7.2",
    "tailwindcss": "1.4.6",
    "ts-node": "8.10.2",
    "typescript": "3.9.5"
  }
}
