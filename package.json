{
  "name": "@mcansh/snkrs",
  "version": "1.0.0",
  "description": "show off your sneaker collection",
  "repository": "mcansh/snkrs",
  "license": "MIT",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "main": "remix.config.js",
  "sideEffects": false,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:remix": "remix dev",
    "dev:server": "cross-env NODE_ENV=development nodemon ./server.mjs",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production run-s build:*",
    "build:icons": "node scripts/heroicon-symbols.mjs",
    "build:remix": "npm run generate:routes && remix build",
    "generate:db": "prisma generate",
    "generate:routes": "routes-gen -d @routes-gen/remix",
    "generate": "run-p generate:*",
    "lint": "eslint --cache .",
    "test": "jest",
    "typecheck": "tsc",
    "validate": "run-p lint type-check test",
    "db:migrate": "prisma migrate dev --preview-feature",
    "prepare": "husky install",
    "clean": "del .cache build public/build app/styles/app.css app/assets/icons/{solid,outline}",
    "start": "cross-env NODE_ENV=production node ./server.mjs",
    "deploy": "flyctl deploy"
  },
  "dependencies": {
    "@headlessui/react": "1.7.10",
    "@mcansh/remix-fastify": "2.4.1",
    "@mcansh/remix-secure-headers": "0.6.2",
    "@prisma/client": "4.10.0",
    "@reach/alert": "0.18.0",
    "@remix-run/node": "0.0.0-nightly-ff22f78-20230208",
    "@remix-run/react": "0.0.0-nightly-ff22f78-20230208",
    "@sentry/node": "7.36.0",
    "accounting": "0.4.1",
    "cloudinary": "1.33.0",
    "cloudinary-build-url": "0.2.4",
    "clsx": "1.2.1",
    "cross-env": "7.0.3",
    "cuid": "2.1.8",
    "date-fns": "2.29.3",
    "etag": "1.8.1",
    "fastify": "4.12.0",
    "fathom-client": "3.5.0",
    "lodash.uniqby": "4.7.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-number-format": "5.1.3",
    "remix-seo": "0.1.0",
    "routes-gen": "0.6.0",
    "slugify": "1.6.5",
    "tiny-invariant": "1.3.1",
    "type-fest": "3.5.6",
    "zod": "3.20.3",
    "zod-form-data": "1.2.4"
  },
  "devDependencies": {
    "@cld-apis/types": "0.1.6",
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@mcansh/eslint-config": "5.0.0",
    "@remix-run/dev": "0.0.0-nightly-ff22f78-20230208",
    "@remix-run/eslint-config": "0.0.0-nightly-ff22f78-20230208",
    "@routes-gen/remix": "0.3.6",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.3",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/accounting": "0.4.2",
    "@types/etag": "1.8.1",
    "@types/jest": "29.4.0",
    "@types/lodash.uniqby": "4.7.7",
    "@types/node": "18.13.0",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "autoprefixer": "10.4.13",
    "cssnano": "5.1.14",
    "del-cli": "5.0.0",
    "dotenv": "16.0.3",
    "eslint": "8.33.0",
    "heroicons": "1.0.6",
    "husky": "8.0.3",
    "jest": "29.4.2",
    "lint-staged": "13.1.1",
    "nodemon": "2.0.20",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.21",
    "postcss-100vh-fix": "1.0.2",
    "postcss-cli": "10.1.0",
    "prettier": "2.8.4",
    "prisma": "4.10.0",
    "remix-flat-routes": "0.4.4",
    "shell-quote": "1.8.0",
    "svgo": "2.8.0",
    "tailwindcss": "3.2.5",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5"
  }
