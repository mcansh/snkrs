{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "predev": "run-s clean build:icons",
    "dev": "run-p dev:*",
    "dev:routes": "npm run generate:routes -- --watch",
    "dev:remix": "cross-env TZ=UTC remix dev",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production run-s build:*",
    "build:icons": "node scripts/heroicon-symbols.mjs",
    "build:remix": "npm run generate:routes && remix build",
    "generate:db": "prisma generate",
    "generate:routes": "remix-routes",
    "generate": "run-p generate:*",
    "lint": "eslint --cache --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --ignore-unknown --write .",
    "test": "jest",
    "typecheck": "tsc",
    "validate": "run-p lint typecheck test",
    "db:migrate": "prisma migrate dev --preview-feature",
    "prepare": "husky install",
    "clean": "./scripts/clean.mjs",
    "start": "cross-env NODE_ENV=production node ./server.mjs"
  },
