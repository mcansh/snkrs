    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "etag": "^1.8.1",
    "fastify": "^4.18.0",
    "fathom-client": "^3.5.0",
    "isbot": "^3.6.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-number-format": "^5.2.2",
    "remix-params-helper": "^0.4.10",
