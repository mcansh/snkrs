{
  "name": "@mcansh/snkrs",
  "version": "1.0.0",
  "description": "my sneaker collection",
  "repository": "mcansh/snkrs",
  "license": "MIT",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "main": "n/a",
  "prettier": "@mcansh/eslint-config/prettier.config",
  "sideEffects": false,
  "scripts": {
    "dev": "pm2-dev pm2.config.js",
    "prebuild": "npm run clean",
    "build": "run-s build:*",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "build:remix": "remix build2",
    "generate": "prisma generate",
    "lint": "eslint --cache --ext .js,.ts,.tsx .",
    "test": "jest",
    "type-check": "tsc --project app",
    "validate": "npm-run-all --parallel lint type-check test",
    "db:migrate": "prisma migrate dev --preview-feature",
    "prepare": "husky install",
    "postinstall": "npm run generate",
    "clean": "del .cache build public/build app/styles",
    "start": "remix-serve ./build"
  },
  "dependencies": {
    "@headlessui/react": "1.0.0",
    "@prisma/client": "2.21.2",
    "@remix-run/express": "0.16.2",
    "@remix-run/node": "0.16.2",
    "@remix-run/react": "0.16.2",
    "@remix-run/serve": "0.16.3",
    "@svgr/webpack": "5.5.0",
    "cloudinary": "1.25.1",
    "clsx": "1.1.1",
    "csurf": "1.11.0",
    "date-fns": "2.21.1",
    "dequal": "2.0.2",
    "express": "4.17.1",
    "express-rate-limit": "5.2.6",
    "express-session": "1.17.1",
    "fathom-client": "3.0.0",
    "formik": "2.2.6",
    "framer-motion": "4.1.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-error-boundary": "3.1.1",
    "react-router": "6.0.0-beta.0",
    "react-router-dom": "6.0.0-beta.0",
    "type-fest": "1.0.2",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-conventional": "12.1.1",
    "@mcansh/eslint-config": "3.0.1",
    "@remix-run/dev": "0.16.2",
    "@testing-library/jest-dom": "5.12.0",
    "@testing-library/react": "11.2.6",
    "@types/csurf": "1.11.1",
    "@types/express-rate-limit": "5.1.1",
    "@types/express-session": "1.17.3",
    "@types/jest": "26.0.22",
    "@types/node": "14.14.41",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/yup": "0.29.11",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "autoprefixer": "10.2.5",
    "concurrently": "6.0.2",
    "del-cli": "3.0.1",
    "dotenv": "8.2.0",
    "eslint": "7.24.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-kentcdodds": "18.0.0",
    "eslint-config-prettier": "8.2.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.11.0",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "npm-run-all": "4.1.5",
    "pm2": "4.5.6",
    "postcss": "8.2.12",
    "postcss-cli": "8.3.1",
    "prettier": "2.2.1",
