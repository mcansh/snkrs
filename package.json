{
  "name": "@mcansh/snkrs",
  "version": "1.0.0",
  "description": "my sneaker collection",
  "repository": "mcansh/snkrs",
  "license": "MIT",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "main": "n/a",
  "prettier": "@mcansh/eslint-config/prettier.config",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && npm run generate"
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "generate": "prisma generate",
    "lint": "eslint --cache --ext .js,.ts,.tsx .",
    "test": "jest",
    "type-check": "tsc",
    "validate": "npm-run-all --parallel lint type-check test",
    "db:save": "prisma migrate save --experimental",
    "db:up": "prisma migrate up --experimental",
    "db:generate": "prisma generate",
    "db:migrate": "npm-run-all db:*"
  },
  "dependencies": {
    "@hapi/joi": "17.1.1",
    "@nexus/schema": "0.13.1",
    "@prisma/client": "2.0.0-beta.6",
    "@svgr/webpack": "5.4.0",
    "argon2": "0.26.2",
    "cloudinary-core": "2.8.2",
    "formik": "2.1.4",
    "next-iron-session": "4.1.6",
    "next": "9.4.4",
    "next-offline": "5.0.2",
    "next-seo": "4.5.0",
    "npm": "6.14.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-simple-img": "2.3.9",
    "swr": "0.2.2",
    "type-fest": "0.15.0"
  },
  "devDependencies": {
    "@types/argon2": "0.15.0",
    "@types/hapi__joi": "17.1.0",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@mcansh/eslint-config": "2.7.1",
    "@prisma/cli": "2.0.0-beta.6",
    "@tailwindcss/ui": "0.3.0",
    "@testing-library/jest-dom": "5.9.0",
    "@testing-library/react": "10.0.4",
    "@types/jest": "25.2.3",
    "@types/node": "14.0.5",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "eslint": "7.1.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-kentcdodds": "14.13.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.10.0",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "lint-staged": "10.2.7",
    "npm-run-all": "4.1.5",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.0.5",
    "shell-quote": "1.7.2",
    "tailwindcss": "1.4.6",
    "ts-node": "8.10.2",
    "typescript": "3.9.3"
  }
}
