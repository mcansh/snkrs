    "date-fns": "2.29.3",
    "date-fns-tz": "2.0.0",
    "etag": "1.8.1",
    "fastify": "4.15.0",
    "fathom-client": "3.5.0",
    "isbot": "3.6.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-number-format": "5.1.4",
    "remix-params-helper": "0.4.10",
    "remix-utils": "6.1.0",
