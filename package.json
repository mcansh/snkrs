{
  "name": "@mcansh/snkrs",
  "version": "1.0.0",
  "description": "show off your sneaker collection",
  "repository": "mcansh/snkrs",
  "license": "MIT",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "main": "remix.config.js",
  "sideEffects": false,
  "scripts": {
    "dev": "pm2-dev pm2.config.js",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production run-s build:*",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "build:icons": "node scripts/heroicon-symbols.mjs",
    "build:remix": "remix build",
    "generate": "prisma generate && routes-gen -d @routes-gen/remix",
    "gen:types": "gen-env-types .env -o app/@types/global.d.ts -e .",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx,.mjs,.cjs,.cts,.mts .",
    "test": "jest --passWithNoTests",
    "type-check": "tsc",
    "validate": "run-p lint type-check test",
    "db:migrate": "prisma migrate dev --preview-feature",
    "prepare": "husky install && remix setup node",
    "clean": "del .cache build public/build app/styles app/icons/{solid,outline}",
    "start": "cross-env NODE_ENV=production node ./build",
    "deploy": "flyctl deploy"
  },
  "dependencies": {
    "@headlessui/react": "1.5.0",
    "@mcansh/remix-fastify": "1.9.1",
    "@prisma/client": "3.10.0",
    "@reach/alert": "0.16.0",
    "@remix-run/node": "1.2.3-pre.0",
    "@remix-run/react": "1.2.3-pre.0",
    "@remix-run/serve": "1.2.3-pre.0",
    "@remix-run/server-runtime": "1.2.3-pre.0",
    "@sentry/node": "6.18.1",
    "accounting": "0.4.1",
    "cloudinary": "1.28.1",
    "cloudinary-build-url": "0.2.4",
    "clsx": "1.1.1",
    "cross-env": "7.0.3",
    "csv-string": "4.1.0",
    "cuid": "2.1.8",
    "date-fns": "2.28.0",
    "etag": "1.8.1",
    "fastify": "3.27.2",
    "fastify-express": "0.3.3",
    "fathom-client": "3.4.1",
    "ioredis": "4.28.5",
    "lodash.uniqby": "4.7.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-number-format": "4.9.1",
    "remix": "1.2.3-pre.0",
    "remix-seo": "0.1.0",
    "routes-gen": "0.3.0",
    "sirv": "2.0.2",
    "slugify": "1.6.5",
    "tiny-invariant": "1.2.0",
    "type-fest": "2.12.0",
    "zod": "3.13.0"
  },
  "devDependencies": {
    "@cld-apis/types": "0.1.6",
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@mcansh/eslint-config": "4.0.0",
    "@remix-run/dev": "1.2.3-pre.0",
    "@remix-run/eslint-config": "1.2.3",
    "@routes-gen/remix": "0.2.10",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/forms": "0.4.0",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.3",
    "@types/accounting": "0.4.2",
    "@types/etag": "1.8.1",
    "@types/ioredis": "4.28.8",
    "@types/jest": "27.4.1",
    "@types/lodash.uniqby": "4.7.6",
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "autoprefixer": "10.4.2",
    "cssnano": "5.0.17",
    "del-cli": "4.0.1",
    "dotenv": "16.0.0",
    "eslint": "8.10.0",
    "gen-env-types": "1.3.2",
    "heroicons": "1.0.5",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.4",
    "npm-run-all": "4.1.5",
    "pm2": "5.2.0",
    "postcss": "8.4.7",
    "postcss-100vh-fix": "1.0.2",
    "postcss-cli": "9.1.0",
    "prettier": "2.5.1",
    "prisma": "3.10.0",
    "shell-quote": "1.7.3",
    "svgo": "2.8.0",
    "tailwindcss": "3.0.23",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  }
}
