{
  "name": "@mcansh/snkrs",
  "version": "1.0.0",
  "description": "my sneaker collection",
  "repository": "mcansh/snkrs",
  "license": "MIT",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "main": "remix.config.js",
  "prettier": "@mcansh/eslint-config/prettier.config",
  "sideEffects": false,
  "scripts": {
    "dev": "pm2-dev pm2.config.js",
    "prebuild": "npm run clean",
    "build": "run-s build:*",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "build:icons": "node scripts/heroicon-symbols.mjs",
    "build:remix": "remix build",
    "generate": "prisma generate",
    "lint": "eslint --cache --ext .js,.ts,.tsx .",
    "test": "jest --passWithNoTests",
    "type-check": "tsc --project app",
    "validate": "npm-run-all --parallel lint type-check test",
    "db:migrate": "prisma migrate dev --preview-feature",
    "prepare": "husky install",
    "postinstall": "npm run generate",
    "clean": "del .cache build public/build app/styles app/icons/{solid,outline}",
    "start": "remix-serve ./build"
  },
  "dependencies": {
    "@headlessui/react": "1.2.0",
    "@prisma/client": "2.24.0",
    "@remix-run/node": "0.17.5",
    "@remix-run/react": "0.17.5",
    "@remix-run/serve": "0.17.5",
    "cloudinary": "1.25.2",
    "cloudinary-core": "2.11.4",
    "clsx": "1.1.1",
    "cuid": "2.1.8",
    "date-fns": "2.22.1",
    "express": "4.17.1",
    "fathom-client": "3.0.0",
    "framer-motion": "4.1.17",
    "ioredis": "4.27.3",
    "lodash.uniqby": "4.7.0",
    "react": "18.0.0-alpha-e6be2d531",
    "react-dom": "18.0.0-alpha-e6be2d531",
    "react-hot-toast": "2.0.0",
    "react-router": "6.0.0-beta.0",
    "react-router-dom": "6.0.0-beta.0",
    "remix": "0.17.5",
    "remix-utils": "1.0.2",
    "reserved-email-addresses-list": "0.0.9",
    "slugify": "1.5.3",
    "type-fest": "1.2.0",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@mcansh/eslint-config": "3.1.0",
    "@remix-run/dev": "0.17.5",
    "@tailwindcss/aspect-ratio": "0.2.1",
    "@tailwindcss/forms": "0.3.2",
    "@testing-library/jest-dom": "5.12.0",
    "@testing-library/react": "11.2.7",
    "@types/ioredis": "4.26.4",
    "@types/jest": "26.0.23",
    "@types/lodash.uniqby": "4.7.6",
    "@types/node": "15.6.2",
    "@types/react": "17.0.9",
    "@types/react-dom": "17.0.6",
    "@types/yup": "0.29.11",
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "autoprefixer": "10.2.6",
    "cssnano": "5.0.5",
    "del-cli": "3.0.1",
    "dotenv": "10.0.0",
    "eslint": "7.27.0",
    "heroicons": "1.0.1",
    "husky": "6.0.0",
    "jest": "27.0.3",
    "lint-staged": "11.0.0",
    "make-dir": "3.1.0",
    "npm-run-all": "4.1.5",
    "pm2": "4.5.6",
    "postcss": "8.3.0",
    "postcss-cli": "8.3.1",
    "prettier": "2.3.0",
    "prisma": "2.24.0",
    "shell-quote": "1.7.2",
    "svgo": "2.3.0",
    "tailwindcss": "2.1.4",
    "ts-jest": "27.0.3",
    "typescript": "4.3.2"
  }
}
