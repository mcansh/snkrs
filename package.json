{
  "name": "@mcansh/snkrs",
  "version": "1.0.0",
  "description": "my sneaker collection",
  "repository": "mcansh/snkrs",
  "license": "MIT",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "main": "n/a",
  "prettier": "@mcansh/eslint-config/prettier.config",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "npm run generate"
    }
  },
  "scripts": {
    "dev:loaders": "tsc -b -w --preserveWatchOutput",
    "dev:css": "postcss styles --base styles --dir app/ -w",
    "dev:remix": "remix run",
    "dev:server": "vercel dev --listen 4000",
    "dev": "concurrently -n w: npm:dev:*",
    "build:tsc": "tsc -b",
    "build:css": "postcss styles --base styles --dir app-build/ --env production",
    "build:remix": "remix build",
    "build": "run-s build:*",
    "start": "node index.js",
    "generate": "prisma generate",
    "lint": "eslint --cache --ext .js,.ts,.tsx .",
    "test": "jest",
    "type-check": "tsc",
    "validate": "npm-run-all --parallel lint type-check test",
    "db:migrate": "prisma migrate dev --preview-feature",
    "postinstall": "npm run generate && patch-package",
    "clean": "del .cache build public/build app-build data-build",
    "predeploy": "npm run clean && npm run build",
    "deploy": "vercel"
  },
  "dependencies": {
    "@headlessui/react": "0.2.0",
    "@prisma/client": "2.13.0",
    "@quixo3/prisma-session-store": "1.1.2",
    "@remix-run/cli": "0.8.3",
    "@remix-run/core": "0.8.3",
    "@remix-run/data": "0.8.3",
    "@remix-run/express": "0.8.3",
    "@remix-run/react": "0.8.3",
    "@svgr/webpack": "5.5.0",
    "argon2": "0.27.1",
    "cloudinary": "1.23.0",
    "cloudinary-core": "2.11.3",
    "clsx": "1.1.1",
    "date-fns": "2.16.1",
    "dequal": "2.0.2",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "fathom-client": "3.0.0",
    "formik": "2.2.6",
    "framer-motion": "3.0.0-rc.16",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router": "6.0.0-beta.0",
    "react-router-dom": "6.0.0-beta.0",
    "type-fest": "0.20.2",
    "yup": "0.32.8"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@mcansh/eslint-config": "3.0.1",
    "@prisma/cli": "2.13.0",
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.2",
    "@types/argon2": "0.15.0",
    "@types/jest": "26.0.19",
    "@types/node": "14.14.14",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/yup": "0.29.10",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "@typescript-eslint/parser": "4.10.0",
    "autoprefixer": "10.1.0",
    "concurrently": "5.3.0",
    "del-cli": "3.0.1",
    "eslint": "7.15.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-kentcdodds": "17.3.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.10.0",
    "husky": "4.3.6",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "npm-run-all": "4.1.5",
    "patch-package": "6.2.2",
    "postcss": "8.2.1",
    "postcss-cli": "8.3.1",
    "prettier": "2.2.1",
    "shell-quote": "1.7.2",
    "tailwindcss": "2.0.2",
    "typescript": "4.1.3"
  }
}
