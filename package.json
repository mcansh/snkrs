{
  "name": "@mcansh/snkrs",
  "version": "1.0.0",
  "description": "my sneaker collection",
  "repository": "mcansh/snkrs",
  "license": "MIT",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "main": "n/a",
  "prettier": "@mcansh/eslint-config/prettier.config",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "npm run generate"
    }
  },
  "scripts": {
    "dev": "remix run",
    "build": "remix build",
    "generate": "prisma generate",
    "lint": "eslint --cache --ext .js,.ts,.tsx .",
    "test": "jest",
    "type-check": "tsc --project app",
    "validate": "npm-run-all --parallel lint type-check test",
    "db:migrate": "prisma migrate dev --preview-feature",
    "postinstall": "husky install && npm run generate",
    "clean": "del .cache build public/build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "0.3.1",
    "@prisma/client": "2.19.0",
    "@remix-run/core": "0.14.1",
    "@remix-run/data": "0.14.1",
    "@remix-run/react": "0.14.1",
    "@remix-run/vercel": "0.14.1",
    "@svgr/webpack": "5.5.0",
    "@vercel/node": "1.9.1",
    "argon2": "0.27.1",
    "cloudinary": "1.25.0",
    "cloudinary-core": "2.11.3",
    "clsx": "1.1.1",
    "connect-redis": "5.1.0",
    "csurf": "1.11.0",
    "date-fns": "2.19.0",
    "dequal": "2.0.2",
    "express": "4.17.1",
    "express-rate-limit": "5.2.6",
    "express-session": "1.17.1",
    "fathom-client": "3.0.0",
    "formik": "2.2.6",
    "framer-motion": "4.0.0",
    "rate-limit-redis": "2.1.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-error-boundary": "3.1.1",
    "react-router": "6.0.0-beta.0",
    "react-router-dom": "6.0.0-beta.0",
    "redis": "3.0.2",
    "type-fest": "0.21.3",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@mcansh/eslint-config": "3.0.1",
    "@prisma/cli": "2.19.0",
    "@remix-run/dev": "0.14.1",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@types/argon2": "0.15.0",
    "@types/csurf": "1.11.0",
    "@types/express-rate-limit": "5.1.1",
    "@types/express-session": "1.17.3",
    "@types/jest": "26.0.21",
    "@types/node": "14.14.35",
    "@types/rate-limit-redis": "1.7.1",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.2",
    "@types/redis": "2.8.28",
    "@types/yup": "0.29.11",
    "@typescript-eslint/eslint-plugin": "4.18.0",
    "@typescript-eslint/parser": "4.18.0",
    "autoprefixer": "10.2.5",
    "concurrently": "6.0.0",
    "del-cli": "3.0.1",
    "eslint": "7.22.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-kentcdodds": "17.5.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.11.0",
    "husky": "5.2.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "npm-run-all": "4.1.5",
    "postcss": "8.2.8",
    "prettier": "2.2.1",
    "shell-quote": "1.7.2",
    "tailwindcss": "2.0.4",
    "typescript": "4.2.3"
  }
}
